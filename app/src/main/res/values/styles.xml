    <style name="WhiskeyTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/newPrimaryColor</item>
        <item name="colorPrimaryDark">@color/newPrimaryColor</item>
        <item name="colorAccent">@color/newPrimaryColor</item>
    </style>

</resources>
